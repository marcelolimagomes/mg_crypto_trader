    "import pandas as pd\n",
    "\n",
    "symbol = 'ETHUSDT'\n",
    "filename = f'../data/{symbol}/{symbol}.csv'\n",
    "df = pd.read_csv(filename, sep=';')\n",
    "df.index = df['open_time']\n",
    "df = df.sort_index()\n",
    "df.drop_duplicates(keep='last', inplace=True)\n",
    "\n",
    "df.to_csv(filename, sep=';', index=False)\n",
    "df.duplicated().sum()"
   ]
  }
